<h1>商品一覧画面</h1>
<div class="container">
    <% @products.each do |product| %>
      <div class="product-block">
          <h5 class="product-name"><%= product.name %></h5>
          <p><%= product.price %></p>
          <p><%= product.id %></p>
          <%= link_to "削除", "/products/#{product.id}/delete", data: { turbo_method: :post } %>
      </div>
    <% end %>
</div>